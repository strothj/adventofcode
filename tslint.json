{
  "extends": [
    "tslint:latest",
    "tslint-eslint-rules",
    "tslint-config-airbnb",
    "tslint-config-prettier"
  ],
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "no-implicit-dependencies": ["true", "dev"],
    "no-console": false,
    "array-type": false,
    "curly": false,
    "variable-name": false,
    "prettier": true
  }
}
